{
  "name": "cowswap-monorepo",
  "version": "2.0.0",
  "description": "CoW Protocol",
  "main": "index.js",
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.12.1",
  "config": {
    "enabledApps": "cowswap-frontend cowswap-frontend-e2e abis ui ui-utils widget-lib widget-react"
  },
  "scripts": {
    "start": "pnpm run start:cowswap",
    "start:cowswap": "cross-env NODE_ENV=development nx run cowswap-frontend:serve",
    "start:widget": "nx run widget-configurator:serve",
    "start:omnibridge-hook": "nx run hook-dapp-omnibridge:serve",
    "start:cosmos": "nx run cowswap-frontend:cosmos:run",
    "start:cosmos:explorer": "nx run explorer:cosmos:run",
    "start:explorer": "cross-env NODE_ENV=development nx run explorer:serve",
    "start:cowfi": "cross-env PORT=3001 nx run cow-fi:dev",
    "start:sdk-tools": "nx run sdk-tools:serve",
    "build": "pnpm run build:cowswap",
    "build:all": "nx run-many -t build",
    "build:cowswap": "cross-env NODE_ENV=production nx build cowswap-frontend",
    "build:explorer": "nx build explorer",
    "build:widget": "nx build widget-configurator",
    "build:cowfi": "nx build cow-fi && pnpm run postbuild:cowfi",
    "build:sdk-tools": "nx build sdk-tools",
    "postbuild:cowfi": "next-sitemap --config ./apps/cow-fi/next-sitemap.config.js",
    "build:cosmos": "nx run cowswap-frontend:build:cosmos",
    "ci:build": "pnpm run build:cowswap && pnpm run build-widget",
    "nx": "npx nx run",
    "ipfs": "nx run cowswap-frontend:ipfs",
    "i18n": "nx run-many -t i18n",
    "preview": "nx run cowswap-frontend:preview",
    "test": "pnpm run test:all",
    "test:all": "nx run-many -t test",
    "e2e": "nx run-many -t e2e",
    "e2e:open": "nx run-many -t e2e:open",
    "lint": "nx run-many -t lint",
    "lint:fix": "pnpm run lint --fix --skip-nx-cache",
    "lint:verbose": "pnpm run lint --output-style=stream --parallel=1 --skip-nx-cache",
    "lint-staged": "lint-staged --verbose -c lint-staged.config.js",
    "prebuild:cowswap": "node tools/scripts/prebuild.js",
    "prepare": "husky",
    "analyze-build": "cross-env ANALYZE_BUNDLE=true ANALYZE_BUNDLE_TEMPLATE=sunburst pnpm run build",
    "install:sdk-preview": "node tools/scripts/install-sdk-preview.js && pnpm install",
    "install:ci": "pnpm run install:sdk-preview",
    "update-sdk-releases": "node tools/scripts/update-sdk-releases.js"
  },
  "browser": {
    "crypto": false
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!@ledgerhq/connect-kit-loader)"
    ]
  },
  "resolutions": {
    "@safe-global/safe-apps-sdk": "^9.1.0",
    "@types/react": "19.1.3",
    "@types/react-dom": "19.1.3",
    "stylus": "npm:empty-npm-package@1.0.0",
    "@babel/traverse": "^7.23.2",
    "@walletconnect/ethereum-provider": "2.18.0",
    "@walletconnect/sign-client": "2.18.0",
    "@walletconnect/universal-provider": "2.18.0",
    "@walletconnect/utils": "2.18.0",
    "@walletconnect/types": "2.18.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.27.0",
    "@lingui/cli": "^5.4.1",
    "@sentry/webpack-plugin": "^2.10.0",
    "@swc/helpers": "~0.5.2",
    "@types/hdkey": "^2.0.1",
    "@walletconnect/ethereum-provider": "^2.18.0",
    "@walletconnect/types": "2.17.3",
    "@web3-react/eip1193": "^8.2.3",
    "@web3-react/empty": "^8.2.3",
    "@web3-react/metamask": "^8.2.4",
    "@web3-react/url": "^8.2.3",
    "next": "15.2.8",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "styled-jsx": "5.1.2",
    "tslib": "^2.3.0",
    "wagmi": "3.1.0"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.14.5",
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@eslint/eslintrc": "^3.1.0",
    "@lingui/swc-plugin": "^5.6.0",
    "@lingui/vite-plugin": "^5.4.1",
    "@nx/cypress": "21.6.4",
    "@nx/eslint": "21.6.4",
    "@nx/eslint-plugin": "21.6.4",
    "@nx/jest": "21.6.4",
    "@nx/js": "21.6.4",
    "@nx/next": "21.6.4",
    "@nx/react": "21.6.4",
    "@nx/vite": "21.6.4",
    "@nx/workspace": "21.6.4",
    "@sentry/vite-plugin": "^4.4.0",
    "@swc-node/register": "~1.11.1",
    "@swc/cli": "~0.7.5",
    "@swc/core": "~1.13.5",
    "@swc/plugin-styled-components": "^9.1.0",
    "@testing-library/dom": "^10.4.0",
    "@typechain/ethers-v5": "^11.1.2",
    "@types/clone-deep": "^4.0.1",
    "@types/d3": "^7.4.0",
    "@types/jest": "^30.0.0",
    "@types/node": "24.7.1",
    "@types/react-is": "19.0.0",
    "@types/wcag-contrast": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^8.4.0",
    "@typescript-eslint/parser": "^8.4.0",
    "@vitejs/plugin-react": "~5.0.4",
    "@vitejs/plugin-react-swc": "^4.1.0",
    "@vitest/coverage-c8": "~0.33.0",
    "@vitest/ui": "~3.2.4",
    "babel-jest": "^30.2.0",
    "babel-plugin-styled-components": "2.1.4",
    "babel-plugin-transform-import-meta": "^2.2.0",
    "eslint": "~9.37.0",
    "eslint-config-next": "15.5.4",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-cypress": "^5.2.0",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-lingui": "^0.10.1",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "7.0.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-unused-imports": "^4.1.3",
    "husky": "^9.1.7",
    "isomorphic-fetch": "^3.0.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-styled-components": "^7.1.1",
    "jest-fetch-mock": "^3.0.3",
    "jsdom": "~27.0.0",
    "lint-staged": "^16.2.7",
    "node-stdlib-browser": "^1.2.0",
    "nx": "21.6.4",
    "next-sitemap": "^4.2.3",
    "patch-package": "^8.0.0",
    "prettier": "^3.3.3",
    "react-cosmos-plugin-vite": "^7.0.0",
    "rollup-plugin-visualizer": "^6.0.4",
    "ts-jest": "^29.1.1",
    "typechain": "^8.3.2",
    "typescript": "~5.9.3",
    "vite": "^7.3.1",
    "vite-plugin-babel-macros": "^1.0.6",
    "vite-plugin-dts": "~4.5.4",
    "vite-plugin-dynamic-import": "^1.5.0",
    "vite-plugin-meta-tags": "^0.1.4",
    "vite-plugin-node-polyfills": "^0.24.0",
    "vite-plugin-pwa": "^1.0.0",
    "vite-plugin-svgr": "^4.2.0",
    "vite-tsconfig-paths": "~5.1.4",
    "vitest": "~3.2.4"
  }
}
