{
  "name": "cowswap-monorepo",
  "version": "2.0.0",
  "description": "CoW Protocol",
  "main": "index.js",
  "author": "",
  "license": "ISC",
  "config": {
    "enabledApps": "cowswap-frontend cowswap-frontend-e2e abis ui ui-utils widget-lib widget-react"
  },
  "scripts": {
    "start": "pnpm run start:cowswap",
    "start:cowswap": "cross-env NODE_ENV=development nx run cowswap-frontend:serve",
    "start:widget": "nx run widget-configurator:serve",
    "start:omnibridge-hook": "nx run hook-dapp-omnibridge:serve",
    "start:cosmos": "nx run cowswap-frontend:cosmos:run",
    "start:cosmos:explorer": "nx run explorer:cosmos:run",
    "start:explorer": "cross-env NODE_ENV=development nx run explorer:serve",
    "start:cowfi": "cross-env PORT=3001 nx run cow-fi:dev",
    "start:sdk-tools": "nx run sdk-tools:serve",
    "build": "pnpm run build:cowswap",
    "build:all": "nx run-many -t build",
    "build:cowswap": "cross-env NODE_ENV=production nx build cowswap-frontend",
    "build:explorer": "nx build explorer",
    "build:widget": "nx build widget-configurator",
    "build:cowfi": "nx build cow-fi && pnpm run postbuild:cowfi",
    "build:sdk-tools": "nx build sdk-tools",
    "postbuild:cowfi": "next-sitemap --config ./apps/cow-fi/next-sitemap.config.js",
    "build:cosmos": "nx run cowswap-frontend:build:cosmos",
    "ci:build": "pnpm run build:cowswap && pnpm run build-widget",
    "nx": "npx nx run",
    "ipfs": "nx run cowswap-frontend:ipfs",
    "i18n": "nx run-many -t i18n",
    "preview": "nx run cowswap-frontend:preview",
    "test": "pnpm run test:all",
    "test:all": "nx run-many -t test",
    "e2e": "nx run-many -t e2e",
    "e2e:open": "nx run-many -t e2e:open",
    "lint": "nx run-many -t lint",
    "lint:fix": "pnpm run lint --fix --skip-nx-cache",
    "lint:verbose": "pnpm run lint --output-style=stream --parallel=1 --skip-nx-cache",
    "lint-staged": "lint-staged --verbose -c lint-staged.config.js",
    "prebuild:cowswap": "pnpm run i18n",
    "prepare": "husky",
    "analyze-build": "cross-env ANALYZE_BUNDLE=true ANALYZE_BUNDLE_TEMPLATE=sunburst pnpm run build",
    "install:sdk-preview": "node tools/scripts/install-sdk-preview.js && pnpm install",
    "install:ci": "pnpm run install:sdk-preview"
  },
  "browser": {
    "crypto": false
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!@ledgerhq/connect-kit-loader)"
    ]
  },
  "resolutions": {
    "@safe-global/safe-apps-sdk": "^9.1.0",
    "@types/react": "19.1.3",
    "@types/react-dom": "19.1.3",
    "stylus": "npm:empty-npm-package@1.0.0",
    "@babel/traverse": "^7.23.2"
  },
  "pnpm": {
    "overrides": {
      "@safe-global/safe-apps-sdk": "^9.1.0",
      "@types/react": "19.1.3",
      "@types/react-dom": "19.1.3",
      "@ethersproject/abi": "5.8.0",
