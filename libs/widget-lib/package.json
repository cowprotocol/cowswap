{
  "name": "@cowprotocol/widget-lib",
  "version": "1.0.0",
  "type": "commonjs",
  "description": "CoW Swap Widget Library. Allows you to easily embed a CoW Swap widget on your website.",
  "main": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "require": "./src/index.ts",
      "default": "./src/index.ts"
    }
  },
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/cowprotocol/cowswap"
  },
  "keywords": [
    "cowprotocol",
    "widget",
    "cowswap",
    "dex",
    "widget-lib"
  ],
  "dependencies": {
    "@cowprotocol/cow-sdk": "7.3.2",
    "@cowprotocol/events": "workspace:*",
    "@cowprotocol/iframe-transport": "workspace:*"
  },
  "types": "./src/index.ts",
  "publishConfig": {
    "main": "./index.js",
    "types": "./index.d.ts",
    "exports": {
      ".": {
        "types": "./index.d.ts",
        "import": "./index.mjs",
        "require": "./index.js"
      }
    }
  }
}
