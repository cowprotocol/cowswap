{
  "name": "@cowprotocol/hook-dapp-lib",
  "version": "1.14.2",
  "type": "commonjs",
  "description": "CoW Swap Hook Dapp Library. Allows you to develop pre/post hooks dapps for CoW Protocol.",
  "main": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "require": "./src/index.ts",
      "default": "./src/index.ts"
    }
  },
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/cowprotocol/cowswap"
  },
  "keywords": [
    "cowprotocol",
    "hooks",
    "cowswap",
    "dex",
    "hook-dapp-lib"
  ],
  "dependencies": {
    "@cowprotocol/iframe-transport": "workspace:*"
  },
  "types": "./src/index.ts",
  "publishConfig": {
    "main": "./index.js",
    "types": "./index.d.ts",
    "exports": {
      ".": {
        "types": "./index.d.ts",
        "import": "./index.mjs",
        "require": "./index.js"
      }
    }
  }
}
