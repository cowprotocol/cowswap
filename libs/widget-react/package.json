{
  "name": "@cowprotocol/widget-react",
  "version": "1.0.2",
  "type": "commonjs",
  "description": "CoW Swap Widget Library. Allows you to easily embed a CoW Swap widget on your React application.",
  "main": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "require": "./src/index.ts",
      "default": "./src/index.ts"
    }
  },
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/cowprotocol/cowswap"
  },
  "keywords": [
    "cowprotocol",
    "widget",
    "cowswap",
    "dex",
    "widget-lib",
    "react"
  ],
  "dependencies": {
    "@cowprotocol/events": "workspace:*",
    "@cowprotocol/types": "workspace:*",
    "@cowprotocol/widget-lib": "workspace:*",
    "react": "19.1.2"
  },
  "types": "./src/index.ts",
  "publishConfig": {
    "access": "public",
    "main": "./index.js",
    "types": "./index.d.ts",
    "exports": {
      ".": {
        "types": "./index.d.ts",
        "import": "./index.mjs",
        "require": "./index.js"
      }
    }
  },
  "devDependencies": {
    "@types/react": "19.1.3"
  }
}
