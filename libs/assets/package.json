{
  "name": "@cowprotocol/assets",
  "version": "2.0.1",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "private": true,
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "require": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./cow-swap/*": "./src/cow-swap/*",
    "./fonts/*": "./src/fonts/*",
    "./images/*": "./src/images/*",
    "./lottie/*": "./src/lottie/*",
    "./svg/*": "./src/svg/*",
    "./video/*": "./src/video/*",
    "./lazy-loaders.ts": "./src/lazy-loaders.ts"
  },
  "publishConfig": {
    "main": "./index.js",
    "types": "./index.d.ts",
    "exports": {
      ".": {
        "types": "./index.d.ts",
        "import": "./index.mjs",
        "require": "./index.js"
      }
    }
  },
  "dependencies": {
    "lottie-react": "^2.4.0",
    "react": "19.1.2"
  }
}
